    <!-- Preference labels -->
    <string name="pref_cat_qemu">QEMU</string>
    <string name="qemu_ram_title">RAM</string>
    <string name="qemu_ram_desc">Maximal amount of RAM that may be allocated for virtual machine.\n\nOptimal values are lower than half of physical RAM available on your device.</string>
    <string name="qemu_hdd1_path_title">Primary hard drive</string>
    <string name="qemu_hdd1_path_desc">Path to a disk image that will be used as primary hard drive. If the image file is not exist, Alpine Term will generate a snapshot of internal hard drive with pre-installed OS.\n\n* This is a default boot device.\n\n* Depending on usage, the image file can grow up to several gigabytes.</string>
    <string name="qemu_hdd1_path_hint">Leave empty to use default.</string>
    <string name="qemu_hdd2_path_title">Secondary hard drive</string>
    <string name="qemu_hdd2_path_desc">Path to a disk image that will be used as secondary drive.\n\n* Can be selected as boot device from bios menu.\n\n* Depending on usage, the image file can grow up to several gigabytes.</string>
    <string name="qemu_hdd2_path_hint">Leave empty to disable.</string>
    <string name="qemu_cdrom_path_title">CD-ROM</string>
    <string name="qemu_cdrom_path_desc">Path to a disk image that will be used as CD-ROM.\nCan be selected as boot device from bios menu.</string>
    <string name="qemu_cdrom_path_hint">Leave empty to disable.</string>
    <string name="qemu_upstream_dns_title">Upstream DNS</string>
    <string name="qemu_upstream_dns_desc">A destination where QEMU will forward all queries from internal DNS server.\n\nGuest OS should be configured to use QEMU\&apos;s internal DNS (10.0.2.3) otherwise this option will not have any effect.</string>
    <string name="qemu_upstream_dns_hint">1.1.1.1</string>
    <string name="qemu_exposed_ports_title">Exposed ports</string>
    <string name="qemu_exposed_ports_desc">A list of port forwarding rules. Each rule has the following format:\n\n{proto}:{external port}:{internal port}\n\nYou can specify multiple rules by separating them with commas.\n\nExternal ports lower than 1024 cannot be used.</string>
    <string name="qemu_exposed_ports_hint">tcp:8080:80, udp:5353:53, etc…</string>

    <!-- Preference dialogs -->
    <string name="pref_dialog_invalid_file_path_title">Invalid file path specified</string>
    <string name="pref_dialog_invalid_file_path_body">One or more components are inaccessible at specified path.\n\nMake sure that file path is correct and you have write permissions.\n\nChanges are discarded.</string>
    <string name="pref_dialog_invalid_dns_ip_title">Invalid DNS IP</string>
    <string name="pref_dialog_invalid_dns_ip_body">You have entered an invalid IPv4 address of the DNS server.\n\nSuggested ones are:\n\n* 1.1.1.1\n* 4.2.2.1\n* 8.8.8.8\n* 206.67.220.220\n\nChanges are discarded.</string>
    <string name="pref_dialog_invalid_port_title">Invalid port number specified</string>
    <string name="pref_dialog_invalid_port_body">You have entered invalid port number.\n\nMinimal allowed port is %1$d and maximal is %2$d.\n\nChanges are discarded.</string>
    <string name="pref_dialog_fwd_invalid_port_title">Forwarding rule error</string>
    <string name="pref_dialog_fwd_invalid_rule">Rule: %s\n\nSyntax error.\n\nRule should be defined in the following format:\n\n(tcp|udp):{1024–65535}:{1–65535}\n\nMultiple rules should be separated by commas.</string>
    <string name="pref_dialog_fwd_invalid_external_port_body">Rule: %s\n\nYou have specified an invalid external port value.\n\nAllowed range for external port is 1024–65535.</string>
    <string name="pref_dialog_fwd_invalid_internal_port_body">Rule: %s\n\nYou have specified an invalid internal port value.\n\nAllowed range for internal port is 1–65535.</string>

